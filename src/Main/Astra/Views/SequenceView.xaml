<UserControl x:Class="Astra.Views.SequenceView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:Astra.UI.Controls;assembly=Astra.UI"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:Astra.Views"
             xmlns:viewmodels="clr-namespace:Astra.ViewModels"
             xmlns:models="clr-namespace:Astra.Models"
             xmlns:converters="clr-namespace:Astra.Converters"
             xmlns:uiConverters="clr-namespace:Astra.UI.Converters;assembly=Astra.UI"
             xmlns:nodeModels="clr-namespace:Astra.Core.Nodes.Models;assembly=Astra.Core"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:behaviors="clr-namespace:Astra.Behaviors"
             xmlns:multiFlowEditor="clr-namespace:Astra.UI.Styles.Controls;assembly=Astra.UI"
             d:DataContext="{d:DesignInstance Type=viewmodels:SequenceViewModel}"
             mc:Ignorable="d"
             Background="{DynamicResource SurfaceBrush}">

    <Grid>
        <!-- 多流程编辑器控件 -->
        <multiFlowEditor:MultiFlowEditor DataContext="{Binding MultiFlowEditor}" />
    </Grid>
</UserControl>
